{
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@2.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "http://michicompany.info",
      "destination": "https://www.michicompany.info",
      "permanent": true
    }
  ]
}